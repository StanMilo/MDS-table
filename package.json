{
  "name": "minimal-json-server-api",
  "version": "1.0.0",
  "description": "Minimal JSON Server API with fake data generation",
  "main": "index.js",
  "scripts": {
    "seed": "tsx scripts/seed.ts",
    "dev": "json-server --watch db.json --port 1008",
    "start": "npm run seed && json-server --watch db.json --port 1008",
    "dev:all": "npm run seed && concurrently --names \"API,FRONTEND\" --prefix-colors \"blue,green\" \"npm run dev\" \"npm run next:dev\"",
    "clean": "rimraf db.json",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start"
  },
  "keywords": [
    "json-server",
    "api",
    "faker"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@faker-js/faker": "^8.3.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/material-nextjs": "^7.3.6",
    "json-server": "^0.17.4",
    "next": "^16.0.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "concurrently": "^9.2.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
